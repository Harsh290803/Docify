generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id       String    @id @default(cuid())
  username String    @unique
  projects Project[]
}

model Project {
  url         String @unique
  userId      String
  testing_dir String
  user        User   @relation(fields: [userId], references: [id])

  @@index([userId])
}
